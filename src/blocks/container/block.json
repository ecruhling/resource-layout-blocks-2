{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "resource-layout-blocks-2/container",
  "title": "Container",
  "category": "resource-layout-blocks-2",
  "description": "A Container/Wrapper; fluid or fixed width. By default, Containers get padding on both sides; half of the gutter value - usually 12px.",
  "keywords": [ "container", "wrapper" ],
  "version": "2.0.0",
  "textdomain": "resource",
  "icon": "screenoptions",
  "attributes": {
    "isFluid": {
      "type": "boolean",
      "default": true
    },
    "tagName": {
      "type": "string",
      "default": "div"
    },
    "inlineStyles": {
      "type": "string"
    }
  },
  "supports": {
    "anchor": true,
    "alignWide": false,
    "background": {
      "backgroundImage": true,
      "backgroundSize": true
    },
    "color": {
      "text": false,
      "background": true
    },
    "dimensions": {
      "aspectRatio": true,
      "height": true,
      "minHeight": true,
      "width": true
    },
    "html": false,
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style.css",
  "editorStyle": "file:./editor.css"
}
